<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.wofford.sykesda.wocoapptest.Auto_discounts">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/imageView6"
        android:layout_below="@+id/imageView6"
        app:srcCompat="@drawable/ic_insurance" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/imageView5"
        android:layout_below="@+id/imageView5"
        app:srcCompat="@drawable/ic_insurance" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/imageView4"
        android:layout_below="@+id/imageView4"
        app:srcCompat="@drawable/ic_insurance" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/imageView3"
        android:layout_below="@+id/imageView3"
        app:srcCompat="@drawable/ic_insurance" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/imageView2"
        android:layout_below="@+id/imageView2"
        app:srcCompat="@drawable/ic_insurance" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="39dp"
        android:layout_marginTop="11dp"
        app:srcCompat="@drawable/ic_insurance" />

    <TextView
        android:id="@+id/textView_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView3"
        android:layout_toEndOf="@+id/imageView4"
        android:text="Allstate"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView3"
        android:text="GEICO"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView_name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2"
        android:text="State Farm"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/monsoon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/monsoon2"
        android:layout_toEndOf="@+id/imageView5"
        android:text="Farmers"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView_name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/lemon_grass2"
        android:layout_toEndOf="@+id/imageView6"
        android:text="Travelers"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/dell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView"
        android:layout_toEndOf="@+id/imageView"
        android:text="Nationwide"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView_description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView3"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView3"
        android:text="Single, full time good student discount upto 20%"
        android:textAlignment="center"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView_description3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView_name"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView4"
        android:text="Full-time B average student ages 16 to 25 save up to 15%"
        android:textAlignment="center"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/monsoon2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView5"
        android:text="Good student discount saving up to 25% before 25"
        android:textAlignment="center"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/lemon_grass2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView6"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView6"
        android:text="Dean's List, Honor Roll, or top 20% student discount"
        android:textAlignment="center"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView_description6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/dell"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView"
        android:text="full students from 16 to 24 get discount"
        android:textAlignment="center"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView_description7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView"
        android:text="Nationwide discount"
        android:textAlignment="center"
        android:textSize="10sp" />


</RelativeLayout>